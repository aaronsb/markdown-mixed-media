{
  "name": "mmm",
  "version": "1.0.0",
  "description": "Mixed Media Markdown - Terminal markdown viewer with sixel graphics support",
  "main": "dist/index-direct.js",
  "bin": {
    "mmm": "./dist/index-direct.js",
    "install-mmm": "./install.js"
  },
  "scripts": {
    "dev": "tsx src/index-direct.tsx",
    "dev:simple": "tsx src/index-simple.tsx",
    "dev:settings": "tsx src/settings-cli.ts",
    "settings": "tsx src/settings-cli.ts",
    "watch": "tsx watch src/index-direct.tsx",
    "start": "node dist/index-direct.js",
    "build": "tsc && chmod +x dist/index-direct.js && chmod +x dist/index-simple.js",
    "build:binary": "npm run build && npm run package",
    "build:simple": "node scripts/build-simple.js",
    "package": "node scripts/build-binary.js",
    "install:local": "npm run build:binary && ./scripts/install.sh",
    "test": "jest"
  },
  "keywords": [
    "markdown",
    "terminal",
    "tui",
    "sixel",
    "mermaid",
    "viewer"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@mermaid-js/mermaid-cli": "^10.8.0",
    "chalk": "^5.3.0",
    "figlet": "^1.7.0",
    "highlight.js": "^11.11.1",
    "marked": "^11.2.0",
    "marked-highlight": "^2.2.2",
    "marked-terminal": "^6.2.0",
    "meow": "^13.1.0",
    "mermaid": "^10.9.0",
    "open": "^10.0.3",
    "puppeteer": "^22.0.0",
    "puppeteer-core": "^24.17.0",
    "sharp": "^0.33.2"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.8",
    "@types/jest": "^29.5.11",
    "@types/marked": "^6.0.0",
    "@types/node": "^20.11.0",
    "esbuild": "^0.19.11",
    "jest": "^29.7.0",
    "nexe": "^4.0.0-rc.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  }
}
