{
  "name": "mmv",
  "version": "0.1.0",
  "description": "Mixed Media Markdown Viewer - Terminal markdown viewer with sixel graphics support",
  "main": "dist/index.js",
  "bin": {
    "mmv": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsx src/index.tsx",
    "dev:simple": "tsx src/index-simple.tsx",
    "dev:direct": "tsx src/index-direct.tsx",
    "watch": "tsx watch src/index.tsx",
    "start": "node dist/index.js",
    "build": "tsc && chmod +x dist/index.js && chmod +x dist/index-simple.js",
    "build:binary": "npm run build && npm run package",
    "package": "node scripts/build-binary.js",
    "install:local": "npm run build:binary && ./scripts/install.sh",
    "test": "jest"
  },
  "keywords": [
    "markdown",
    "terminal",
    "tui",
    "sixel",
    "mermaid",
    "viewer"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ink": "^4.4.1",
    "ink-link": "^3.0.0",
    "ink-markdown": "^1.0.2",
    "marked": "^11.2.0",
    "marked-terminal": "^6.2.0",
    "mermaid": "^10.9.0",
    "@mermaid-js/mermaid-cli": "^10.8.0",
    "puppeteer": "^22.0.0",
    "sharp": "^0.33.2",
    "chalk": "^5.3.0",
    "figlet": "^1.7.0",
    "meow": "^13.1.0",
    "open": "^10.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.48",
    "@types/marked": "^6.0.0",
    "@types/figlet": "^1.5.8",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "nexe": "^4.0.0-rc.2",
    "esbuild": "^0.19.11",
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0"
  },
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  }
}